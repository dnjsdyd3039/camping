<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.camily.dao.BoardDao" >

<!-- 게시판 insert -->
<insert id="insertBoard" parameterType="com.camily.dao.BoardDao">

	INSERT INTO BOARDS (BOCODE, BOMID, BOTITLE, BOCONTENTS)
				VALUES (#{bocode}, #{bomid}, #{botitle}, #{bocontents})
</insert>



</mapper>